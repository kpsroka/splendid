{
  "name": "splendid",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.1",
    "react-scripts": "^1.0.14",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-test-id": "^1.0.2",
    "babel-preset-react-app": "^3.0.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.2.0",
    "connect-history-api-fallback": "1.4.0",
    "cross-spawn": "5.1.0",
    "css-loader": "0.28.7",
    "detect-port": "1.2.1",
    "dotenv": "4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "4.9.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "filesize": "3.5.11",
    "flow": "^0.2.3",
    "flow-bin": "^0.57.3",
    "fs-extra": "4.0.2",
    "gzip-size": "4.0.0",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-middleware": "0.17.4",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "json-loader": "0.5.7",
    "object-assign": "4.1.1",
    "path-exists": "3.0.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "react-dev-utils": "^4.1.0",
    "react-test-renderer": "^16.0.0",
    "recursive-readdir": "2.2.1",
    "redux-devtools": "^3.3.1",
    "sinon": "^4.0.1",
    "strip-ansi": "4.0.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.2",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node run/scripts/start.js",
    "build": "node run/scripts/build.js",
    "test": "node run/scripts/test.js --env=jsdom",
    "flow": "flow"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/run/config/jest/shim.js",
      "<rootDir>/run/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/run/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/run/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "production": {
        "plugins": [
          ["react-test-id", {"props": ["testId"]}]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
